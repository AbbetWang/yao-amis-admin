{
    "label": "下载文件",
    "version": "1.0.0",
    "description": "下载文件",
    "nodes": [
        {
            "description": "获取公共文件下载地址",
            "name": "filename",
            "process": "scripts.system.file.getPublicFilePath",
            "args": [
                "{{$in.0}}"
            ]
        },
        {
            "name": "下载文件",
            "process": "fs.system.Download",
            "args": [
                "{{$res.filename}}"
            ]
        }
    ],
    "output": "{{$res.下载文件}}"
}